<% title "Channels" %>

<table>
  <tr>
    <th>Name</th>
    <th>Series</th>
  </tr>
  <% for channel in @channels %>
    <tr>
      <td><%= channel.name %></td>
      <td>  
        <%= raw(listify(channel.series_items)) %>
      </td>
      <td><%= link_to "Show", channel %></td>
    </tr>
  <% end %>
</table>
