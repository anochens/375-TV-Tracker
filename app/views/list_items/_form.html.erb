<%= form_for @list_item do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :user_id %><br />
    <%= f.collection_select :user_id,User.all, :id, :to_s %>
  </p>
  <p>
    <%= f.label :series_item_id %><br />
    <%= f.collection_select :series_item_id,SeriesItem.all, :id, :to_s %>
  </p>
  <p>
    <%= f.label :watched %><br />
    <%= f.check_box :watched %>
  </p>
  <p><%= f.submit %></p>
<% end %>
